import sys

args = sys.argv

csv_file = open(args[1], "r")
f = csv_file.readlines()
csv_file.close()

output = open(args[2],"w")

for i in f:
    i = i.replace("\n","")
    i = i.split(",")
    del i[6]
    del i[4]
    del i[2]
    del i[1]
    print(i)
    tmp = ','.join(i)
    tmp = tmp+"\n"
    output.write(tmp)
