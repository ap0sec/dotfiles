var i = 0;

function init(){
    setInterval(function(){controllImage1()},17);
}

function controllImage1(){
    i = ((i) % 500) + 1;

    if(i > 120){
        var path = "data/logo/image1.png"
    }else{
        var path = "data/logo/" + i + ".gif"
    }
    document.querySelector('#image-1').setAttribute('src', path)
}
