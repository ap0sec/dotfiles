<virtualHost *:80>
    ServerName educations.prane.biz
    DocumentRoot /var/www/html/educations
    AllowEncodedSlashes On
    CustomLog      logs/educations.access.log combined
    ErrorLog       logs/educations.error.log
    <Directory /var/www/html/educations>
        SetEnv REAL_HOST_NAME educations.prane.biz
        AllowOverride All
        Require all granted
    </Directory>
</VirtualHost>